{
  "version": 2,
  "builds": [
    {
      "src": "dist/click-reviews-angular-front/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/click-reviews-angular-front/server/server.mjs"
    }
  ],
  "buildCommand": "npm run build:ssr",
  "outputDirectory": "dist/click-reviews-angular-front/browser"
}
